{
  "manifest_version": 3,
  "name": "JSON viewer & jq",
  "description": "WIP",
  "version": "1.0",
  "action": {
    "default_icon": "icon-test.png"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
